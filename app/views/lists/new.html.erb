<div class="container mt-3">
  <h1>Add List</h1>
  <%= simple_form_for(@list) do |f| %>
      <%= f.input :name %>
      <%= f.input :photo, as: :file %>
      <%= f.submit 'Submit', class: 'btn btn-primary mt-2' %>
  <% end %>
  <%= link_to root_path, class: 'btn btn-primary mt-3' do %>
      <i class="fa-solid fa-angles-left"></i>
    <% end %>
</div>
